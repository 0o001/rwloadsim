# test full stack with error locations

$startseconds:0

integer one := 1, two := 2, zero := 0;
double threads sum half := 0.0;

procedure zerodiv()
  one := two / zero;
end;

procedure div()
  half := half + one / two;
end;

procedure callzd()
  zerodiv();
end;

run
  threads 1 
    for count 1 loop zerodiv(); end;
  end threads;

  threads 2
    for count 2 loop div(); end;
  end threads;

  threads 1
    for count 2 loop callzd(); end;
  end threads;
end run;

printline half;

$include:"100-1.rwl"
